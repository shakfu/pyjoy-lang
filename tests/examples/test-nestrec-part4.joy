2 setecho.
0 __settracegc.

(* x-ack *)
DEFINE
    twice-x ==
        dup [x] dip x.
DEFINE
    x-ack ==
        [ [ [ [pop pop null]  pop popd succ ]
            [ [pop null]  [pop pred 1] dip x ]
            [ [[dup pred swap] dip pred] dip twice-x ] ]
        cond ]
        x.

[ [3 0] [3 1] [3 2] [3 3] [3 4] [3 5] ]   [i x-ack]  map.
